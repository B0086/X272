#include <windows.h>
#include <iostream>

// Exe dosyasının raw verileri
unsigned char exeData[] = {
    // Exe dosyasının raw verilerini buraya hex formatında ekleyin
    // Örnek:
    0x4D, 0x5A, 0x90, 0x00, 0x03, 0x00, 0x00, 0x00, 0x04, 0x00, 0x00, 0x00, ...
};

int main() {
    // Başlatma parametresi
    LPCSTR parameters = "XXXX";

    // Bellekteki exe dosyasını çalıştır
    HINSTANCE hInstance = ShellExecuteA(NULL, "open", reinterpret_cast<LPCSTR>(&exeData), parameters, NULL, SW_SHOWNORMAL);
    if ((int)hInstance <= 32) {
        std::cerr << "Exe çalıştırılamadı!" << std::endl;
        return 1;
    }

    return 0;
}
