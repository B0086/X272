using System;
using System.Diagnostics;

class Program
{
    static void Main()
    {
        string sourceIpAddress = "45.13.227.206";
        string destinationIpAddress = "185.205.19.150";

        string command = $"New-NetRoute -DestinationPrefix {sourceIpAddress} -NextHop {destinationIpAddress}";
        
        ExecutePowerShellCommand(command);
    }

    static void ExecutePowerShellCommand(string command)
    {
        ProcessStartInfo psi = new ProcessStartInfo()
        {
            FileName = "powershell.exe",
            Arguments = $"-Command \"{command}\"",
            UseShellExecute = false,
            RedirectStandardOutput = true,
            CreateNoWindow = true
        };

        using (Process process = Process.Start(psi))
        {
            if (process != null)
            {
                string output = process.StandardOutput.ReadToEnd();
                process.WaitForExit();
            }
        }
    }
}
